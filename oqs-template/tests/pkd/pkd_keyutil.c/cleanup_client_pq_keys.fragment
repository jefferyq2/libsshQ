{%- for sig in config['sigs'] %}
    {%- for variant in sig['variants'] %}
        cleanup_key(OPENSSH_{{ variant['name']|upper }}_TESTKEY);
        {%- for classical_alg in variant['mix_with'] %}
        cleanup_key(OPENSSH_{{ classical_alg['name']|upper }}_{{ variant['name']|upper }}_TESTKEY);
        {%- endfor %}
    {%- endfor %}
{%- endfor %}

