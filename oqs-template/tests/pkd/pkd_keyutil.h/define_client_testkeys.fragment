{%- for sig in config['sigs'] %}
    {%- for variant in sig['variants'] %}
#define OPENSSH_{{ variant['name']|upper }}_TESTKEY "openssh_testkey.id_{{ variant['name']|lower }}"
        {%- for classical_alg in variant['mix_with'] %}
#define OPENSSH_{{ classical_alg['name']|upper }}_{{ variant['name']|upper }}_TESTKEY "openssh_testkey.id_{{ classical_alg['name']|lower }}_{{ variant['name']|lower }}"
        {%- endfor %}
    {%- endfor %}
{%- endfor %}

