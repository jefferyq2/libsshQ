{%- for sig in config['sigs'] %}
    {%- for variant in sig['variants'] %}
    PKDTESTS_OQSKEYAUTH(emit_testmap, openssh_{{ variant['name']|lower }}, OPENSSH_KEX_CMD)
        {%- for classical_alg in variant['mix_with'] %}
    PKDTESTS_OQSKEYAUTH(emit_testmap, openssh_{{ classical_alg['name']|lower }}_{{ variant['name']|lower }}, OPENSSH_KEX_CMD)
        {%- endfor %}
    {%- endfor %}
{%- endfor %}

